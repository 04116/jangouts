(function () {
  'use strict';

  angular.module("<%- moduleName %>", [])
    .provider('jhConfig', function () {
      var config = {
        <% constants.forEach(function(constant, i) { %>
        "<%- constant.name %>": <%= constant.value %><% if (i < constants.length - 1) { %>,<% } %>
        <% }); %>
      };

      return {
        $get: function () {
          return config;
        }
      };
    });
})();
